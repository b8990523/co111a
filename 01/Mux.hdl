/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    // Put your code here:
    //此程式為自行完成
    Not(in=sel,out=nots);
    And(a=a,b=nots,out=notsanda);
    And(a=sel,b=b,out=sandb);
    Or(a=notsanda,b=sandb,out=out);
}
